@using Swallow.Blazor.Reactive.Abstractions
@using Swallow.Blazor.Reactive.Components
@attribute [ReactiveComponent]

@for (var i = 0; i < Count; ++i)
{
    <div>
        @i:
        <ReactiveSection Name="@i.ToString()">
            <ButtonListButton/>
        </ReactiveSection>
    </div>
}

@code {
    [Parameter]
    public int Count { get; set; } = 0;
}
